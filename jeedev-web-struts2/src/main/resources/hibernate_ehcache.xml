<ehcache>  
    <diskSore path="cache"/>
	<defaultCache 
		maxElementsInMemory="10000" 
		eternal="false"
		overflowToDisk="true" 
		timeToIdleSeconds="120" 
		timeToLiveSeconds="120"
		diskPersistent="false" 
		diskExpiryThreadIntervalSeconds="120" />
	<!-- acegi cache-->
	<cache name="people" 
		maxElementsInMemory="500" 
		eternal="true"
		overflowToDisk="true" />
</ehcache>
